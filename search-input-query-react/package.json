{
  "name": "search-input-query-react",
  "version": "0.1.2",
  "keywords": [
    "search",
    "parser",
    "query",
    "typescript"
  ],
  "author": "William Cotton",
  "repository": {
    "type": "git",
    "url": "https://github.com/williamcotton/search-input-query"
  },
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/SearchInputQuery.d.ts",
      "import": "./dist/esm/SearchInputQuery.js",
      "require": "./dist/cjs/SearchInputQuery.js"
    }
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "search-input-query-parser": "^0.1.2"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "search-input-query-parser": "^0.1.2",
    "sql.js": "^1.12.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/sql.js": "^1.4.9",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10"
  },
  "license": "ISC"
}
